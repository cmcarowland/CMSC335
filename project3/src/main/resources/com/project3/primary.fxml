<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.event.ActionEvent?>
<?import javafx.scene.control.Slider?>

<GridPane gridLinesVisible="false" style="-fx-background-color:rgb(20, 29, 23);"
         xmlns="http://javafx.com/javafx/8.0.171" 
         xmlns:fx="http://javafx.com/fxml/1" >
   <columnConstraints>
      <ColumnConstraints percentWidth="25" hgrow="ALWAYS" />
      <ColumnConstraints percentWidth="25" hgrow="ALWAYS" />
      <ColumnConstraints percentWidth="25" hgrow="ALWAYS" />
      <ColumnConstraints percentWidth="25" hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints percentHeight="33" vgrow="ALWAYS" />
      <RowConstraints percentHeight="33" vgrow="ALWAYS" />
      <RowConstraints percentHeight="33" vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <Button fx:id="btn1" text="Stop" GridPane.columnIndex="0" GridPane.rowIndex="0" />
      <Button fx:id="btn1" text="Pause" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      <Button fx:id="btn1" text="Play" GridPane.columnIndex="2" GridPane.rowIndex="0" />
      <Button fx:id="btn1" text="Continue" GridPane.columnIndex="3" GridPane.rowIndex="0" />
      <Label text="Light 1" GridPane.columnIndex="0" GridPane.rowIndex="1" />
   </children>
</GridPane>